{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "lab",
	"scopeName": "source.lab",
	"patterns": [
		{
			"include": "#pipeline"
		},
		{
			"include": "#import"
		},
		{
			"include": "#vars"
		},
		{
			"include": "#config"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"embedded": {
			"patterns": [
				{
					"begin": "```(js|javascript|nodejs)((:[a-zA-Z0-9._\\-/@]*)|)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.javascript.lab",
					"patterns": [
						{
							"include": "source.js"
						}
					]
				},
				{
					"begin": "```typescript((:[a-zA-Z0-9._\\-/@]*)|)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.typescript.lab",
					"patterns": [
						{
							"include": "source.ts"
						}
					]
				},
				{
					"begin": "```json((:[a-zA-Z0-9._\\-/@]*)|)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.json.lab",
					"patterns": [
						{
							"include": "source.json"
						}
					]
				},
				{
					"begin": "```go((:[a-zA-Z0-9._\\-/@]*)|)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.go.lab",
					"patterns": [
						{
							"include": "source.go"
						}
					]
				},
				{
					"begin": "```(yml|yaml)((:[a-zA-Z0-9._\\-/@]*)|)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.yaml.lab",
					"patterns": [
						{
							"include": "source.yaml"
						}
					]
				},
				{
					"begin": "```(html|text)((:[a-zA-Z0-9._\\-/@]*)|)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.html.lab",
					"patterns": [
						{
							"include": "text.html.derivative"
						}
					]
				},
				{
					"begin": "```(md|markdown)((:[a-zA-Z0-9._\\-/@]*)|)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.html.lab",
					"patterns": [
						{
							"include": "text.html.markdown"
						}
					]
				},
				{
					"begin": "```python((:[a-zA-Z0-9._\\-/@]*)|)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.python.lab",
					"patterns": [
						{
							"include": "source.python"
						}
					]
				},
				{
					"begin": "(```rhai((:[a-zA-Z0-9._\\-/@]*)|)|```)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.rhai.lab",
					"patterns": [
						{
							"include": "source.rhai"
						}
					]
				},
				{
					"begin": "```php((:[a-zA-Z0-9._\\-/@]*)|)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.php.lab",
					"patterns": [
						{
							"include": "source.php"
						}
					]
				},
				{
					"begin": "```(sh|bash|shell)((:[a-zA-Z0-9._\\-/@]*)|)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.shell.lab",
					"patterns": [
						{
							"include": "source.shell"
						}
					]
				},
				{
					"begin": "```xml((:[a-zA-Z0-9._\\-/@]*)|)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.xml.lab",
					"patterns": [
						{
							"include": "text.xml"
						}
					]
				},
				{
					"begin": "```hbs((:[a-zA-Z0-9._\\-/@]*)|)\n",
					"end": "```",
					"beginCaptures": {
						"0": {
							"name": "punctuation.code.open.lab"
						}
					},
					"endCaptures": {
						"0": {
							"name": "punctuation.code.close.lab"
						}
					},
					"name": "meta.embedded.block.hbs.lab",
					"patterns": [
						{
							"include": "text.html.handlebars"
						}
					]
				}
			]
		},
		"comments": {
			"patterns": [
				{
					"comment": "Single line comments with double-slash",
					"match": "(\/\/)+.*",
					"captures": {
						"0": {
							"name": "comment.line.double-slash.lab"
						}
					}
				},
				{
					"comment": "Multiple line comment block",
					"begin": "/\\*",
					"beginCaptures": {
						"0": {
							"name": "comment.block.documentation.lab"
						}
					},
					"end": "\\*/",
					"endCaptures": {
						"0": {
							"name": "comment.block.documentation.lab"
						}
					},
					"contentName": "comment.block.documentation.lab"
				}
			]
		},
		"pipeline": {
			"patterns": [
				{
					"begin": "pipeline\\s*\\{",
					"captures": {
						"0": {
							"name": "support.type.lab"
						}
					},
					"end": "\\}",
					"patterns": [
						{
							"include": "$self"
						},
						{
							"include": "#modules"
						},
						{
							"include": "#interpolation"
						}
					]
				}
			]
		},
		"import": {
			"patterns": [
				{
					"begin": "import\\s*\\{",
					"captures": {
						"0": {
							"name": "support.type.lab.import"
						}
					},
					"end": "\\}",
					"patterns": [
						{
							"include": "$self"
						},
						{
							"include": "#macro"
						},
						{
							"include": "#param"
						}
					]
				}
			]
		},
		"vars": {
			"patterns": [
				{
					"begin": "vars\\s*\\{",
					"captures": {
						"0": {
							"name": "support.type.lab"
						}
					},
					"end": "\\}",
					"patterns": [
						{
							"include": "$self"
						},
						{
							"include": "#param"
						},
						{
							"include": "#interpolation"
						},
						{
							"include": "#macro"
						}
					]
				}
			]
		},
		"config": {
			"patterns": [
				{
					"begin": "config\\s*\\{",
					"captures": {
						"0": {
							"name": "support.type.lab"
						}
					},
					"end": "\\}",
					"patterns": [
						{
							"include": "$self"
						},
						{
							"include": "#param"
						},
						{
							"include": "#embedded"
						},
						{
							"include": "#interpolation"
						},
						{
							"include": "#macro"
						}
					]
				}
			]
		},
		"modules": {
			"patterns": [
				{
					"begin": "([\\w-]+)((\\s([\\w-]+))|)\\s*\\{",
					"beginCaptures": {
						"0": {
							"name": "entity.name.function.lab"
						},
						"1": {
							"name": "storage.type.struct.lab"
						}
					},
					"end": "\\}",
					"patterns": [
						{
							"include": "$self"
						},
						{
							"include": "#macro"
						},
						{
							"include": "#attach"
						},
						{
							"include": "#param"
						},
						{
							"include": "#interpolation"
						}
					]
				}
			]
		},
		"macro": {
			"patterns": [
				{
					"begin": "([\\w-]+)\\s*(\\s*(true|false|[0-9.]|\".*\"|\\[.*\\]|\\{.*\\}|\\$\\{.*\\})|)*\\s*\\(",
					"beginCaptures": {
						"0": {
							"name": "entity.name.function.macro.lab",
							"patterns": [
								{
									"include": "#numeric"
								},
								{
									"include": "#constant"
								},
								{
									"include": "#boolean"
								},
								{
									"include": "#array"
								},
								{
									"include": "#map"
								},
								{
									"include": "#string"
								},
								{
									"include": "#interpolation"
								},
								{
									"include": "#string_interpolation"
								}
							]
						},
						"1": {
							"name": "entity.name.function.macro.lab"
						}
					},
					"end": "\\)",
					"patterns": [
						{
							"include": "$self"
						},
						{
							"include": "#param"
						},
						{
							"include": "#attach"
						},
						{
							"include": "#macro"
						}
					]
				}
			]
		},
		"attach": {
			"patterns": [
				{
					"begin": "([|]|lab)\\s*([\\w-]+)((\\s*([\\w-]+))|)\\s*\\{",
					"end": "}",
					"beginCaptures": {
						"0": {
							"name": "entity.name.function.lab"
						},
						"1": {
							"name": "keyword.control.lab"
						},
						"2": {
							"name": "storage.type.struct.lab"
						}
					},
					"patterns": [
						{
							"include": "$self"
						},
						{
							"include": "#macro"
						},
						{
							"include": "#attach"
						},
						{
							"include": "#param"
						},
						{
							"include": "#interpolation"
						}
					]
				},
				{
					"match": "([|]|lab)\\s*([\\w-]+)",
					"captures": {
						"1": {
							"name": "keyword.control.lab"
						},
						"0": {
							"name": "entity.name.function.lab"
						}
					},
					"patterns": [
						{
							"include": "$self"
						}
					]
				},
				{
					"match": "([|]|lab)\\s*([\\w-]+)",
					"captures": {
						"1": {
							"name": "keyword.control.lab"
						},
						"0": {
							"name": "entity.name.function.lab"
						}
					},
					"patterns": [
						{
							"include": "$self"
						}
					]
				},
				{
					"match": "([|]|lab)\\s*([@][\\w-]+)",
					"captures": {
						"1": {
							"name": "keyword.control.lab"
						},
						"0": {
							"name": "variable.language.self.lab"
						}
					},
					"patterns": [
						{
							"include": "$self"
						}
					]
				}
			]
		},
		"param": {
			"patterns": [
				{
					"begin": "([\\w\\.-]+)\\s*(=|:)\\s*",
					"beginCaptures": {
						"1": {
							"name": "variable.other.property.lab"
						},
						"2": {
							"name": "keyword.operator.lab"
						}
					},
					"end": "(?<!\\2)$",
					"endCaptures": {
						"0": {
							"name": "keyword.operator.lab"
						}
					},
					"patterns": [
						{
							"include": "#numeric"
						},
						{
							"include": "#constant"
						},
						{
							"include": "#boolean"
						},
						{
							"include": "#array"
						},
						{
							"include": "#map"
						},
						{
							"include": "#embedded"
						},
						{
							"include": "#string"
						},
						{
							"include": "#interpolation"
						},
						{
							"include": "#string_interpolation"
						},
						{
							"include": "#struct"
						}
					]
				}
			]
		},
		"struct": {
			"patterns": [
				{
					"comment": "Types and Structs",
					"match": "[A-Z]([a-z]*[a-zA-Z0-9_]?)?",
					"captures": {
						"0": {
							"name": "entity.name.class.lab"
						}
					}
				}
			]
		},
		"interpolation": {
			"patterns": [
				{
					"begin": "\\$\\{",
					"beginCaptures": {
						"0": {
							"name": "keyword.operator.lab"
						}
					},
					"end": "\\}",
					"endCaptures": {
						"0": {
							"name": "keyword.operator.lab"
						}
					},
					"patterns": [
						{
							"include": "source.rhai"
						}
					]
				}
			]
		},
		"array": {
			"patterns": [
				{
					"begin": "\\[",
					"end": "\\]",
					"patterns": [
						{
							"include": "#numeric"
						},
						{
							"include": "#constant"
						},
						{
							"include": "#boolean"
						},
						{
							"include": "#string"
						},
						{
							"include": "#interpolation"
						},
						{
							"include": "#map"
						},
						{
							"include": "$self"
						},
						{
							"include": "#string_interpolation"
						}
					]
				}
			]
		},
		"map": {
			"patterns": [
				{
					"begin": "\\{",
					"end": "\\}",
					"patterns": [
						{
							"begin": "(\"[\\w\\s\\.-]+\")\\s*(:)",
							"beginCaptures": {
								"1": {
									"name": "string.quoted.double.lab"
								},
								"2": {
									"name": "keyword.operator.lab"
								}
							},
							"end": "(?<!\\2)\\s*($|(?=\\}))",
							"endCaptures": {
								"0": {
									"name": "keyword.operator.lab"
								}
							},
							"patterns": [
								{
									"include": "#numeric"
								},
								{
									"include": "#constant"
								},
								{
									"include": "#boolean"
								},
								{
									"include": "#string"
								},
								{
									"include": "#interpolation"
								},
								{
									"include": "#array"
								},
								{
									"include": "$self"
								},
								{
									"include": "#string_interpolation"
								}
							]
						}
					]
				}
			]
		},
		"numeric": {
			"patterns": [
				{
					"comment": "Numbers in decimal with optional suffixes",
					"match": "\\b[0-9\\.]+([kKmMgG]b?)?\\b",
					"captures": {
						"0": {
							"name": "constant.numeric.lab"
						}
					}
				}
			]
		},
		"boolean": {
			"patterns": [
				{
					"match": "\\b(true|false)\\b",
					"captures": {
						"0": {
							"name": "constant.language.boolean.lab"
						}
					}
				}
			]
		},
		"string": {
			"patterns": [
				{
					"comment": "Usual string",
					"begin": "\"",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.string.begin.lab"
						}
					},
					"end": "\"",
					"endCaptures": {
						"0": {
							"name": "punctuation.definition.string.end.lab"
						}
					},
					"patterns": [
						{
							"match": "[^\"]",
							"captures": {
								"0": {
									"name": "string.quoted.double.lab"
								}
							}
						}
					]
				},
				{
					"comment": "Alt string",
					"begin": "'",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.string.begin.lab"
						}
					},
					"end": "'",
					"endCaptures": {
						"0": {
							"name": "punctuation.definition.string.end.lab"
						}
					},
					"patterns": [
						{
							"match": "[^']",
							"captures": {
								"0": {
									"name": "string.quoted.double.lab"
								}
							}
						}
					]
				}
			]
		},
		"string_interpolation": {
			"patterns": [
				{
					"comment": "String with interpolation",
					"begin": "`",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.string.begin.lab"
						}
					},
					"end": "`",
					"endCaptures": {
						"0": {
							"name": "punctuation.definition.string.end.lab"
						}
					},
					"patterns": [
						{
							"include": "#interpolation"
						},
						{
							"match": "[^`]",
							"captures": {
								"0": {
									"name": "string.quoted.double.lab"
								}
							}
						}
					]
				}
			]
		}
	}
}